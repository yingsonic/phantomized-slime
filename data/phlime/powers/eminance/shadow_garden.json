                                                        "type": "origins:command",
                                                        "command": "execute if entity @s[tag=inGarden]",
                                                        "comparison": "==",
                                                        "compare_to": 1
                                                    }
                                                },
                                                "if_action": {
                                                    "type": "origins:add_velocity",
                                                    "y": 0.3,
                                                    "z": -1,
                                                    "set": true
                                                },
                                                "else_action": {
                                                    "type": "origins:add_velocity",
                                                    "y": 0.3,
                                                    "z": 1,
                                                    "set": true
                                                }
                                            }
                                        ]
                                    },
                                    "bientity_condition": {
                                        "type": "origins:and",
