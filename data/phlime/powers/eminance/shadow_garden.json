{
    "name": "§5§nShadow Garden",
    "description": "If primary is used with a sword in main hand, enlight the surrounding area in a purple light and buff your damage, speed, and reach when in the area. No entities will be able to enter nor exit the Shadow Garden while it is active. The Shadow Garden has a 50 second cooldown may be dispelled by pressing your primary a second time with a sword in hand or on death.\n",
    "hidden": false,

    "type": "origins:multiple",
    "sword": {
        "type": "apoli:active_self",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:resource",
                        "resource": "*:*_var",
                        "comparison": "==",
                        "compare_to": 0
                    },
                    "if_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:execute_command",
                                "command": "summon minecraft:armor_stand ~ ~ ~ {Marker:1b,Invisible:1b,PersistenceRequired:1b,Invulnerable:1b,NoGravity:1b,Tags:[\"tempGarden\"]}"
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "function phlime:sid/garden"
                            },
                            {
                                "type": "apoli:change_resource",
                                "resource": "*:*_var",
                                "operation": "set",
                                "change": 1
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "tag @s add inGarden"
                            },
                            {
                                "type": "apoli:area_of_effect",
                                "bientity_action": {
                                    "type": "origins:target_action",
                                    "action": {
                                        "type": "origins:execute_command",
                                        "command": "tag @s add inGarden"
                                    }
                                },
                                "include_target": false,
                                "radius": 25
                            }
                        ]
                    },
                    "else_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "apoli:execute_command",
                                "command": "execute as @s at @s as @e if score @p gardenSID = @s gardenSID run kill @s[type=minecraft:armor_stand]"
                            },
                            {
                                "type": "origins:trigger_cooldown",
                                "power": "*:*_cd"
                            },
                            {
                                "type": "apoli:change_resource",
                                "resource": "*:*_var",
                                "operation": "set",
                                "change": 0
                            },
                            {
                                "type": "apoli:area_of_effect",
                                "bientity_action": {
                                    "type": "origins:target_action",
                                    "action": {
                                        "type": "origins:execute_command",
                                        "command": "tag @s remove inGarden"
                                    }
                                },
                                "include_target": false,
                                "radius": 30
                            }
                        ]
                    }
                }
            ]
        },
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "apoli:equipped_item",
                    "equipment_slot": "mainhand",
                    "item_condition": {
                        "type": "apoli:ingredient",
                        "ingredient": {
                            "tag": "phlime:swords"
                        }
                    }
                },
                {
                    "type": "origins:resource",
                    "resource": "*:*_cd",
                    "comparison": "==",
                    "compare_to": 0
                }
            ]
        },
        "hud_render": {
            "should_render": false
        },
        "key": {
            "key": "key.origins.primary_active"
        },
        "cooldown": 20
    },
    "disallow": {
        "type": "apoli:action_over_time",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
