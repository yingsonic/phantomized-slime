{
    "name": "§5§nAdaptive Biology",
    "description": "You are evolved, and as a slime are able to modify your molecular structure to aid you in battle. Switch states with the corresponding keybinds noted in the badge.\n",
    "hidden": false,

    "badges": [
        {
            "type": "origins:keybind",
            "sprite": "origins:textures/gui/badge/active.png",
            "text": "§5Switch to Rogue by using [Active Power (Ternary)]\nSwitch to Combat by using [Active Power (Quaternary)]\nSwitch to Guardian by using [Active Power (Quinary)]\n\n§9§lCooldown: 5s"
        }
    ],

    "type": "apoli:multiple",
    "rogue": {
        "type": "apoli:active_self",
        "entity_action": {
            "type": "apoli:if_else_list",
            "actions": [
                {
                    "condition": {
                        "type": "apoli:resource",
                        "resource": "*:*_cd",
                        "comparison": "==",
                        "compare_to": 0,
                        "inverted": true
                    },
                    "action": {
                        "type": "origins:nothing"
                    }
                },
                {
                    "condition": {
                        "type": "apoli:resource",
                        "resource": "*:*_var",
                        "comparison": "==",
                        "compare_to": 1,
                        "inverted": true
                    },
                    "action": {
                        "type": "apoli:and",
                        "actions": [
                            {
                                "type": "apoli:change_resource",
                                "resource": "*:*_var",
                                "operation": "set",
                                "change": 1
                            },
                            {
                                "type": "apoli:trigger_cooldown",
                                "power": "*:*_cd"
                            }
                        ]
                    }
                },
                {
                    "condition": {
                        "type": "apoli:constant",
                        "value": true
                    },
                    "action": {
                        "type": "apoli:execute_command",
                        "command": "title @s actionbar \"§cAlready selected!\""
                    }
                }
            ]
        },
        "key": {
            "key": "key.origins.ternary_active"
        }
    },
    "combat": {
        "type": "apoli:active_self",
        "entity_action": {
            "type": "apoli:if_else_list",
            "actions": [
                {
                    "condition": {
                        "type": "apoli:resource",
                        "resource": "*:*_cd",
                        "comparison": "==",
                        "compare_to": 0,
                        "inverted": true
                    },
                    "action": {
                        "type": "origins:nothing"
                    }
                },
                {
                    "condition": {
                        "type": "apoli:resource",
                        "resource": "*:*_var",
                        "comparison": "==",
                        "compare_to": 2,
                        "inverted": true
                    },
                    "action": {
                        "type": "apoli:and",
                        "actions": [
                            {
                                "type": "apoli:change_resource",
                                "resource": "*:*_var",
                                "operation": "set",
                                "change": 2
                            },
                            {
                                "type": "apoli:trigger_cooldown",
                                "power": "*:*_cd"
                            }
                        ]
                    }
                },
                {
                    "condition": {
                        "type": "apoli:constant",
