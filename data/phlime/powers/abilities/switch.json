        }
    },
    "var": {
        "type": "origins:resource",
        "min": 0,
        "max": 3,
        "start_value": 1,
        "hud_render": {
            "should_render": false,
            "sprite_location": "origins:textures/gui/community/spiderkolo/resource_bar_02.png",
            "bar_index": 21
        }
    },
    "cd": {
        "type": "apoli:cooldown",
        "cooldown": 60,
        "hud_render": {
            "should_render": true,
            "sprite_location": "origins:textures/gui/community/spiderkolo/resource_bar_03.png",
            "bar_index": 9
        }
    }
