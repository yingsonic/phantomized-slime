                    },
                    "action": {
                        "type": "apoli:execute_command",
                        "command": "title @s actionbar \"§cAlready selected!\""
                    }
                }
            ]
        },
        "key": {
            "key": "key.origins.quaternary_active"
        }
    },
    "guardian": {
        "type": "apoli:active_self",
